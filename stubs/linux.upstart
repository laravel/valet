description "Laravel Valet for Ubuntu"
author "Nil Portugues Caldero <contact@nilportugues.com>"

script
    /usr/bin/php -S 127.0.0.1:80 SERVER_PATH
end script

pre-start script
    echo "[`date`] laravel-valetd Starting"
end script

pre-stop script
    rm /var/run/nodetest.pid
    echo "[`date`] laravel-valetd Stopping"
end script